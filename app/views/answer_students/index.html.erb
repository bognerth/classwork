<h1>Listing question_students</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Student</th>
    <th>Test</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @question_students.each do |question_student| %>
  <tr>
    <td><%= question_student.question_id %></td>
    <td><%= question_student.student_id %></td>
    <td><%= question_student.test_id %></td>
    <td><%= link_to 'Show', question_student %></td>
    <td><%= link_to 'Edit', edit_question_student_path(question_student) %></td>
    <td><%= link_to 'Destroy', question_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

